<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled" value="true"/><!-- 全局映射器启用缓存 -->
        <setting name="useGeneratedKeys" value="true" /><!--生成key-->
        <setting name="defaultExecutorType" value="REUSE"/><!--默认Excutor为ReuseExcutor-->
    </settings>

    <typeAliases>
        <!-- 分页 -->
        <typeAlias type="com.laile.esf.common.util.Page" alias="Page"/>
    </typeAliases>

    <plugins>
        <plugin interceptor="com.laile.volcano.core.plugin.PagePlugin">
            <property name="dialect" value="MySQL"/>
        </plugin>
        <plugin interceptor="com.laile.volcano.core.plugin.ResultPlugin">
            <property name="dialect" value="MySQL"/>
        </plugin>
    </plugins>
</configuration>